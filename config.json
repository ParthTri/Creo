{
	"Python": {
		"inherit": false,
		"projectsDir": "~/Developer/",
		"git": true,
		"gitignore": "__pycache__\n.venv/",
		"env": false,
		"externalProgramsEnd": [
			"python3 -m venv .venv"
		],
		"dirs": [
			"tests",
			"src",
			"docs",
			"docs/examples"
		],
		"files": [
			"docs/Purpose.org",
			"README.org"
		]
	},
	"React": {
		"inherit": false,
		"git": false,
		"env": false,
		"externalProgramsEnd": [
			"pnpm install"
		],
		"externalProgramsStart": [
			"pnpm create vite $name --template react",
			"cd $path && pnpm install"
		]
	}
}
